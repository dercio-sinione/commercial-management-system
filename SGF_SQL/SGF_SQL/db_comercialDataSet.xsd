                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        (SELECT        id
                          FROM            venda_pagamento AS vpp
                          WHERE        (id = f.id_pagamento)) AS idd, p.cliente, pr.nome, v.quantidade, v.preco, v.quantidade * v.preco AS total_p, v.data_venda, v.id_funcionario, p.total, p.valor_pago, p.tipo_pagamento, 
