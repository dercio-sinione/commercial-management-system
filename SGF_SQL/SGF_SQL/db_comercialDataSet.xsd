              <DbSource ConnectionRef="db_comercialConnectionString (Settings)" DbObjectName="db_comercial.dbo.vw_produtos" DbObjectType="View" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        categoria, nome, preco,
                             (SELECT        SUM(quantidade) AS Expr1
                               FROM            vw_vendas AS v
                               WHERE        (id_produto = p.id) AND (YEAR(data) = YEAR(CURRENT_TIMESTAMP))
                               GROUP BY produto) AS 'n_vendas',
                             (SELECT        SUM(total) AS Expr1
                               FROM            vw_vendas AS v
                               WHERE        (id_produto = p.id) AND (YEAR(data) = YEAR(CURRENT_TIMESTAMP))
                               GROUP BY produto) AS 'Total'
FROM            vw_produtos AS p
WHERE        ((SELECT        SUM(quantidade) AS Expr1
                            FROM            vw_vendas AS v
                            WHERE        (id_produto = p.id) AND (YEAR(data) = YEAR(CURRENT_TIMESTAMP))
                            GROUP BY produto) IS NOT NULL)
ORDER BY 'n_vendas' DESC, categoria;    
</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
